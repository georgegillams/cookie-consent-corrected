{
  "name": "react-redux-boilerplate",
  "version": "1.0.0",
  "description": "A minimal react-redux boilerplate with all the best practices",
  "repository": {
    "type": "git",
    "url": "git://github.com/flexdinesh/react-redux-boilerplate.git"
  },
  "engines": {
    "npm": ">=3",
    "node": ">=5"
  },
  "author": "Dinesh Pandiyan",
  "license": "MIT",
  "scripts": {
    "migrate-code": "node scripts/copy-api.js && node scripts/copy-components.js --working-tree-unclean",
    "transpile-bpk-dependencies": "node ./scripts/transpile-bpk-dependencies.js",
    "postinstall": "npm run transpile-bpk-dependencies && npm run build",
    "prebuild": "npm run build:clean && npm run link-helpers",
    "build": "npm run build:client && npm run build:server",
    "build:server": "NODE_ENV=development babel -d ./build/server ./server -s",
    "build:client": "cross-env NODE_PATH=./app NODE_ENV=production webpack --config config/webpack.prod.babel.js --color -p --progress --hide-modules --display-optimization-bailout",
    "build:clean": "rimraf ./build",
    "link-helpers": "ln -Ffs $(pwd)/helpers $(pwd)/app/helpers && ln -Ffs $(pwd)/helpers $(pwd)/server/api/helpers",
    "start:dev": "cross-env NODE_PATH=./server/api:./app NODE_ENV=development babel-node server",
    "start:production": "npm run test && npm run build && npm run start:prod",
    "start": "npm run start:prod",
    "start:prod": "cross-env NODE_PATH=./build/server/api NODE_ENV=production node ./build/server",
    "dev": "SECRET_API_KEY=asdfghjkl npm run start:dev",
    "clean": "npm run test:clean && npm run build:clean",
    "prettier": "prettier --config .prettierrc --write \"**/*.js\"",
    "lint": "npm run lint:js",
    "lint:eslint": "eslint --ignore-path .gitignore",
    "eslint:fix": "eslint --ignore-path .gitignore -- . --fix",
    "lint:js": "npm run lint:eslint -- . ",
    "pretest": "npm run test:clean && npm run lint",
    "test:clean": "rimraf ./coverage",
    "test": "cross-env NODE_ENV=test jest --coverage",
    "test:watch": "cross-env NODE_ENV=test jest --watchAll"
  },
  "dependencies": {
    "babel-polyfill": "6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "bcrypt": "^3.0.2",
    "bibtex-parse-js": "0.0.24",
    "bpk-component-checkbox": "^1.4.52",
    "bpk-component-code": "^1.1.27",
    "bpk-component-icon": "^4.0.4",
    "bpk-component-image": "^2.0.61",
    "bpk-component-input": "^4.1.36",
    "bpk-component-label": "^3.2.91",
    "bpk-component-progress": "^1.0.91",
    "bpk-component-text": "^1.0.87",
    "bpk-component-textarea": "^1.0.85",
    "bpk-mixins": "^18.0.4",
    "bpk-theming": "^1.1.14",
    "bpk-tokens": "^27.2.0",
    "chalk": "^2.3.2",
    "compression": "1.7.4",
    "cookie-parser": "^1.4.3",
    "cross-env": "^5.1.6",
    "express": "4.17.1",
    "express-session": "^1.15.6",
    "fontfaceobserver": "2.1.0",
    "funnies": "^2.0.0",
    "history": "4.9.0",
    "hoist-non-react-statics": "3.3.0",
    "immutable": "3.8.2",
    "invariant": "2.2.4",
    "ip": "1.1.5",
    "lodash": "^4.17.10",
    "minimist": "1.2.0",
    "moment": "^2.22.2",
    "nodemailer": "^6.3.0",
    "pretty-error": "^2.1.1",
    "prop-types": "^15.6.1",
    "react": "^16.4.0",
    "react-component-academic-reference": "^1.0.5",
    "react-cookies": "^0.1.0",
    "react-dom": "^16.4.0",
    "react-helmet": "5.2.1",
    "react-loadable": "^5.4.0",
    "react-loading": "^2.0.3",
    "react-redux": "7.1.1",
    "react-responsive-modal": "^4.0.1",
    "react-router-dom": "^5.0.1",
    "react-router-redux": "5.0.0-alpha.6",
    "redis": "^2.8.0",
    "redux": "4.0.4",
    "redux-immutable": "4.0.0",
    "redux-saga": "1.0.5",
    "reselect": "4.0.0",
    "safe-compare": "^1.1.3",
    "sanitize.css": "11.0.0",
    "scroll-into-view": "^1.9.3",
    "socket.io": "^2.1.1",
    "warning": "^4.0.1",
    "whatwg-fetch": "3.0.0",
    "youtube-embed-video": "^1.1.0"
  },
  "devDependencies": {
    "add-asset-html-webpack-plugin": "3.1.3",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "10.0.3",
    "babel-loader": "8.0.6",
    "babel-plugin-add-module-exports": "^1.0.0",
    "babel-plugin-dynamic-import-node": "2.3.0",
    "babel-plugin-react-transform": "3.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-plugin-transform-react-constant-elements": "6.23.0",
    "babel-plugin-transform-react-inline-elements": "6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.24",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "circular-dependency-plugin": "5.2.0",
    "css-loader": "3.2.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^6.2.2",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-skyscanner-with-prettier": "^0.8.0",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-plugin-flowtype": "^4.2.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "^7.9.1",
    "eslint-plugin-redux-saga": "^1.1.0",
    "eventsource-polyfill": "0.9.6",
    "exports-loader": "0.7.0",
    "file-loader": "4.2.0",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "image-webpack-loader": "^5.0.0",
    "imports-loader": "0.8.0",
    "jest-cli": "^24.9.0",
    "lint-staged": "^9.2.4",
    "node-plop": "^0.19.0",
    "node-sass": "^4.7.2",
    "null-loader": "3.0.0",
    "plop": "2.4.0",
    "react-test-renderer": "^16.4.0",
    "rimraf": "3.0.0",
    "sass-loader": "^7.0.1",
    "shelljs": "^0.8.1",
    "style-loader": "^1.0.0",
    "url-loader": "2.1.0",
